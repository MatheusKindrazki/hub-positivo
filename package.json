{
  "name": "hub",
  "private": true,
  "version": "1.9.1",
  "repository": {
    "type": "git",
    "url": "https://editorati.visualstudio.com/Produtos%20Digitais/_git/HubDigitalFront"
  },
  "author": {
    "email": "matheussaldanha@positivosolucoes.com.br",
    "name": "Matheus Kindrazki"
  },
  "badges": [
    "hub",
    "novo-on",
    "positivo-on"
  ],
  "workspaces": {
    "packages": [
      "packages/*",
      "packages/apps/*"
    ]
  },
  "scripts": {
    "test": "jest --runInBand -u",
    "test:watch": "jest --runInBand --watch -u --collectCoverage",
    "test:coverage": "jest --collectCoverage -u --testResultsProcessor=jest-sonar-reporter && coverage-badges",
    "posttest:coverage": "svg2png --input './docs/**/*.svg' --output ./docs/icons --format png",
    "lint": "eslint --ext .tsx,.ts, packages/**/src",
    "build:web": "npm run --prefix packages/apps/hub-web build",
    "release": "HUSKY_SKIP_HOOKS=1 semantic-release"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@hub/eslint-config": "*",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@testing-library/react-hooks": "^5.0.3",
    "@types/jest": "^26.0.20",
    "commitizen": "^4.2.3",
    "coverage-badges": "^1.0.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "~4.3.0",
    "jest": "^26.6.3",
    "jest-matchmedia-mock": "^1.1.0",
    "jest-sonar-reporter": "^2.0.0",
    "jest-svg-transformer": "^1.0.0",
    "lint-staged": "^10.5.4",
    "react-select-event": "^5.2.0",
    "semantic-release": "^17.3.8",
    "semantic-release-monorepo": "^7.0.4",
    "svg-png-converter": "^0.0.9",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {}
}
