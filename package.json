{
  "name": "hub",
  "private": true,
  "version": "1.2.2",
  "repository": {
    "type": "git",
    "url": "https://editorati.visualstudio.com/Produtos%20Digitais/_git/HubDigitalFront"
  },
  "author": {
    "email": "matheussaldanha@positivosolucoes.com.br",
    "name": "Matheus Kindrazki"
  },
  "badges": [
    "hub",
    "novo-on",
    "positivo-on"
  ],
  "workspaces": {
    "packages": [
      "packages/*",
      "packages/apps/*"
    ]
  },
  "scripts": {
    "test": "jest --passWithNoTests --runInBand -u",
    "test:watch": "jest --passWithNoTests --runInBand --watch -u",
    "lint": "eslint --ext .tsx,.ts, packages/**/src",
    "build:web": "npm run --prefix packages/apps/hub-web build",
    "build:header": "npm run --prefix packages/apps/header-inject build"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@hub/eslint-config": "*",
    "@testing-library/react-hooks": "^5.0.3",
    "@types/jest": "^26.0.20",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "~4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@sentry/react": "^6.1.0",
    "@sentry/tracing": "^6.1.0",
    "source-map-explorer": "^2.5.2"
  }
