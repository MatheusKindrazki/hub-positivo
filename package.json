{
  "name": "hub-positivo",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-push": "react-app-rewired test --watchAll false"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --config prettier.config.js --write",
      "eslint --fix --ignore-pattern '!.eslintrc.json'",
      "git add -u"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "^~/(.*)": "<rootDir>/src/$1"
    },
    "collectCoverageFrom": [
      "src/pages/**/*.tsx",
      "src/modules/**/*.ts",
      "src/components/**/*.tsx"
    ]
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build:dev": "cross-env PUBLIC_URL=https://appsv-hubdigitalfront-dev-001.azurewebsites.net react-app-rewired build",
    "build:hml": "cross-env PUBLIC_URL=https://api2.positivoon.com.br/hubdigital-front-hml react-app-rewired build",
    "test": "react-app-rewired test",
    "coverage": "yarn test --coverage --watchAll false",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@chakra-ui/core": "^0.8.0",
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@unform/core": "^2.1.3",
    "@unform/web": "^2.1.3",
    "apisauce": "^1.1.2",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.8.0",
    "cross-env": "^7.0.2",
    "date-fns": "^2.16.1",
    "emotion-theming": "^10.0.27",
    "history": "4.10.1",
    "immer": "^7.0.9",
    "lodash": "^4.17.20",
    "phosphor-react": "^1.0.0",
    "polished": "^3.6.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-responsive": "^8.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-select": "^3.1.0",
    "react-spinners": "^0.9.0",
    "react-toastify": "^6.0.9",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-encrypt": "^3.0.1",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.2.0",
    "ts-debounce": "^2.0.1",
    "typescript": "^4.0.3",
    "uuidv4": "^6.2.3",
    "yup": "^0.29.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/classnames": "^2.2.10",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.11.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-responsive": "^8.0.2",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-select": "^3.0.22",
    "@types/react-virtualized-select": "^3.0.8",
    "@types/redux": "^3.6.0",
    "@types/styled-components": "^5.1.3",
    "@types/yup": "^0.29.7",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-plugin-root-import": "^6.5.0",
    "commitizen": "^4.2.1",
    "customize-cra": "^1.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.1",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "react-app-rewired": "^2.1.6",
    "redux-mock-store": "^1.5.4"
  }
}
